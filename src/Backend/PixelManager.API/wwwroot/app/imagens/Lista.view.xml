<mvc:View
    controllerName="pixelmanager.app.imagens.Lista"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core">

    <Page id="listaPage" title="{i18n>imageListPageTitle}">
        <customHeader>
            <OverflowToolbar>
                <Title text="{i18n>appTitle}" level="H1" />
            </OverflowToolbar>
        </customHeader>

        <content>
            <VBox class="sapUiMediumMargin">
                <core:Fragment fragmentName="pixelmanager.app.fragmentos.ListaToolbar"
                    type="XML" />
                <core:Fragment fragmentName="pixelmanager.app.fragmentos.ListaTabela"
                    type="XML" />
            </VBox>
        </content>
    </Page>
</mvc:View>
